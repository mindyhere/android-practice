<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/folder"
        android:label="@string/app_name"
        android:roundIcon="@drawable/folder"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ex04"
        tools:targetApi="31">
        <activity
            android:name=".ExcelLoadActivity"
            android:exported="false" />
        <activity
            android:name=".ExcelSaveActivity"
            android:exported="false" />
        <activity
            android:name=".ExcelActivity"
            android:exported="false" />
        <activity
            android:name=".RestoreActivity"
            android:exported="false" />
        <activity
            android:name=".BackupActivity"
            android:exported="false" />
        <activity
            android:name=".ExternalActivity"
            android:exported="false" />
        <activity
            android:name=".ReadWriteActivity"
            android:exported="false" />
        <activity
            android:name=".ResourceActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>